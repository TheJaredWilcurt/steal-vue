{
  "name": "steal-vue",
  "version": "0.1.1",
  "description": "A Vue.js Single File Component loader for StealJS",
  "main": "steal-vue.js",
  "repository": "https://github.com/icanjs/steal-vue",
  "author": "Marshall Thompson - marshall@creativeideal.net",
  "license": "MIT",
  "dependencies": {
    "vue-template-compiler": "^2.6.6"
  },
  "steal": {
    "ext": {
      "vue": "steal-vue"
    },
    "plugins": [
      "steal-css",
      "steal-less",
      "steal-vue"
    ]
  },
  "devDependencies": {
    "steal": "^2.1.14",
    "steal-css": "^1.3.2",
    "steal-less": "^1.3.4",
    "vue": "^2.6.6"
  }
}
