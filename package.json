{
  "name": "steal-vue",
  "version": "0.1.1",
  "description": "A Vue.js Single File Component loader for StealJS",
  "main": "steal-vue.js",
  "repository": "https://github.com/icanjs/steal-vue",
  "author": "Marshall Thompson - marshall@creativeideal.net",
  "license": "MIT",
  "scripts": {
    "start": "http-server"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.11.1",
    "steal": "^2.1.14",
    "steal-css": "^1.3.2",
    "steal-sass": "^1.1.1",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.6.6"
  },
  "steal": {
    "ext": {
      "vue": "steal-vue"
    },
    "plugins": [
      "steal-css",
      "steal-sass",
      "steal-vue"
    ]
  }
}
